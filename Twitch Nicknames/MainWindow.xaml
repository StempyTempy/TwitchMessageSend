<Window x:Class="Twitch_Nicknames.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Twitch_Nicknames"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="300" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0" Background="#FF18181B">
    <Grid Margin="11,0,9,0">
        <Grid.RowDefinitions>
            <RowDefinition MinHeight="32" MaxHeight="50" />
            <RowDefinition MinHeight="32" MaxHeight="50" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="Hello Gumbo"></TextBlock>
        <TextBlock Grid.Row="1" Text="Hello Bumbo"></TextBlock>
    </Grid>
</Window>
